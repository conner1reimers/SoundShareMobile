{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx"
  },
  "include": [
    "./Components/**/*.tsx",
    "./**/*.tsx",
    "./types",
    "./types/**/*.ts",
    "./types/declarations.d.ts",
    "./types/types.d.ts",
    "./redux/store/**/*.js"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
  ]

}
